        'HTML5',
        'CSS3',
        'React',
        'Sass',
        'Less',
        'Webpack',
        'Bootstrap',
        'UI/UX',
        'SEO',
        'Gulp',
        'Handlebars',
        'PhotoShop',
        'Illustrator',
      ]
    },
    {
      title: 'BackEnd',
      skillList: [
